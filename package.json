    "vue-multiselect": "^2.0.6",
    "vue-onsenui": "2.2.x",
    "vue2-leaflet": "0.0.54",
    "vuex": "^2.4.0"
  },
  "devDependencies": {
    "babel-core": "^6.0.0",
    "babel-loader": "^6.0.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-latest": "^6.0.0",
